pipeline {
    agent any 

    stages {
        stage('clonings') { 
            steps {
               echo "cloning"
               git branch: 'main', credentialsId: 'jenkinsTest', url: 'https://github.com/Oualitsen/optidial-front.git'
            }
        }
 stage('build web') { 
            steps {
               echo "build"
               sh"cd $WORKSPACE" 
               sh"flutter build web"
            
            }
        }
 
    }
}
